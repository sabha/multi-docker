{
	"AWSEbDockerrunVersion": 2,
	"containerDefinitions": [{
			"name": "client",
			"image": "sabhab1/multi-client",
			"hostname": "client",
			"essential": false,
			"memory": 128
		},
		{
			"name": "server",
			"image": "sabhab1/multi-server",
			"hostname": "api",
			"essential": false,
			"memory": 128
		},
		{
			"name": "worker",
			"image": "sabhab1/multi-worker",
			"hostname": "worker",
			"essential": false,
			"memory": 128
		},
		{
			"name": "nginx",
			"image": "sabhab1/multi-nginx",
			"hostname": "nginx",
			"essential": true,
			"memory": 128,
			"portMappings": [{
				"hostPort": 80,
				"containerPort": 80
			}],
			"links": ["client", "server"]
		}
	]
}